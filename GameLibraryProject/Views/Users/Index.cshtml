@{
    ViewBag.Title = "Users";
}
<h2 class="text-center mt-5 mb-5">@ViewBag.Title</h2>
<table class="table table-dark">
    <thead>
    <th>Username</th>
    <th>Email</th>
    <th>Update</th>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.UsersList)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@Html.ActionLink("Update", "Edit", new { id = user.Id })</td>
                
            </tr>
        }
    </tbody>
</table>
